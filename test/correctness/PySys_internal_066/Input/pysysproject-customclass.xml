<?xml version="1.0" standalone="yes"?>
<pysysproject>
	<property environment="env"/>

	<property osfamily="osfamily"/>

	<property name="verboseWaitForSignal" value="true"/>
	<property name="defaultAbortOnError" value="true"/>

	<property name="perfparent" value="perfsummary"/>

	<path value="${root}/extrapath"/>
	<formatters>
		<formatter name="runlog" customopt="CUSTOM_RUNLOG_PREFIX" classname="mypkg.customfmt.CustomFormatter"/>
		<formatter name="stdout" customopt="CUSTOM_STDOUT_PREFIX" module="mypkg.customfmt" classname="CustomFormatter"/>
	</formatters>
	
</pysysproject>
