<?xml version="1.0" standalone="yes"?>
<pysystest type="auto" state="runnable">

  <description> 
    <title>Performance test for calculating the fibonacci series</title>
    <purpose><![CDATA[
Demonstrate how to measure and report performance results by measuring 
the time taken to calculate fibonacci numbers. 
]]>
    </purpose>
  </description>

  <classification>
    <groups inherit="true">
      <group>examples</group>
      <group>performance</group>
      
      <!-- Always disable coverage reporting for performance tests -->
      <group>disableCoverage</group>
    </groups>
     <modes inherit="true">
        <mode>FibonacciMode1</mode>
        <mode>FibonacciMode2</mode>
    </modes>

  </classification>

  <data>
    <class name="PySysTest" module="run"/>
  </data>
  
  <traceability>
    <requirements>  
      <requirement id="Performance" />    
    </requirements>
  </traceability>
  
</pysystest>

