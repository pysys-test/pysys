<?xml version="1.0" standalone="yes"?>
<pysysproject>
	<property environment="env"/>

	<property osfamily="osfamily"/>

	<property name="shutdownApamaComponentsAfterTest" value="true"/>
	<property name="verboseWaitForSignal" value="true"/>
	<property name="defaultAbortOnError" value="true"/>

	<property name="perfparent" value="perfsummary"/>

	<path value="${root}/extrapath"/>
	<performancereporter classname="CustomPerfReporter" module="mypkg.customperf" summaryfile="${perfparent}/@OUTDIR@_@TESTID@_perf_@HOSTNAME@_@DATE@_@TIME@.csv"/>
	
</pysysproject>
